{
    "extends": "tslint-config-airbnb",
    "rules": {
        "object-shorthand-properties-first": false,
        "no-increment-decrement": false,
        "import-name": false,
        "trailing-comma": false,
        "prefer-template": false,
        "no-else-after-return": false,
        "ter-computed-property-spacing": false,
        "max-line-length": [
            true,
            200
        ],
        "import-blacklist": [
            true,
            ".",
            ".."
        ]
    }
}
